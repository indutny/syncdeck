{
  "version": 2,
  "name": "syncdeck",
  "builds": [
    {"src": "package.json", "use": "@now/static-build"}
  ],
  "routes": [
    {"src": "^/js/(.*)", "dest": "/js/$1"},
    {"src": "^/css/(.*)", "dest": "/css/$1"},
    {"src": "^/images/(.*)", "dest": "/images/$1"},
    {"src": "^/(.*\\.(?:js|txt|json|png))", "dest": "/$1"},
    {"src": ".*", "dest": "/index.html"}
  ]
}
